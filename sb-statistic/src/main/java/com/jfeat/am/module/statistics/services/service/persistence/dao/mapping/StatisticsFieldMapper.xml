<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.statistics.services.service.persistence.dao.StatisticsFieldMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.statistics.services.service.persistence.model.StatisticsField">
		<id column="id" property="id" />
		<result column="field" property="field" />
		<result column="group_name" property="groupName" />
		<result column="name" property="name" />
		<result column="pattern" property="pattern" />
		<result column="chart" property="chart" />
		<result column="attr_invisible" property="attrInvisible" />
		<result column="attr_index" property="attrIndex" />
		<result column="attr_percent" property="attrPercent" />
		<result column="runtime" property="runtime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, field, group_name AS groupName, name, pattern, chart, attr_invisible AS attrInvisible, attr_index AS attrIndex, attr_percent AS attrPercent, runtime
    </sql>

</mapper>
